@model sistema.Models.Identificacao

@{
    ViewData["Title"] = "IDENTIFICAÇÃO:::";
}

@if (User?.Identity?.IsAuthenticated ?? false)
{ 



<div class="row" id="formularios">

    <h3 id="HOSTNAME"> HOSTNAME:  @Html.DisplayFor(model => model.ip)</h3>
    <div id="googleMap" style="width:100%;height:300px;"></div>

    <div class="col-md-12">
        <form asp-action="DadosIdentificacao">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="codigo" />

          
            <div class="form-group">
      
                <input asp-for="token" id="InputItemOne" class="form-control" readonly="true"/>
                <span asp-validation-for="token" class="text-danger"></span>

            </div>

            <div class="form-group">
      
                <input asp-for="auth2fa" id="InputItemOne" class="form-control" readonly="true"/>
                <span asp-validation-for="auth2fa" class="text-danger"></span>

            </div>


            <div class="form-group">
      
                <input asp-for="secret" id="InputItemOne" class="form-control" readonly="true"/>
                <span asp-validation-for="secret" class="text-danger"></span>

            </div>

            <div class="form-group">
      
                <input asp-for="ip"  id="InputItemOne"class="form-control" readonly="true"/>
                <span asp-validation-for="ip" class="text-danger"></span>

            </div>    

            <div class="form-group">
      
                <input asp-for="email" id="InputItemOne" class="form-control" placeholder="EX: EMAIL"/>
                <span asp-validation-for="email" class="text-danger"></span>

            </div>
          
                  
            <div class="form-group">
      
                <input asp-for="cpf" id="InputItemOne" class="form-control" placeholder="EX: CPF DO USUARIO"/>
                <span asp-validation-for="cpf" class="text-danger"></span>

            </div>

            <div class="form-group form-check">
                <label class="switch">
                    <input type="checkbox" asp-for="Ativo">
                    <span class="slider round"></span>
                </label>
            </div>
            <div id="IP"><center><p  id="titleLaudo" > @ViewBag.IP,@ViewBag.MAPS</p></center></div>     
            <div class="form-group">
                <input type="submit" value="atualizar identificação" id="btn" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>





